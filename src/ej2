let habilidades: string[] = ['Bash','counter','Healing'];

interface Personaje {
    nombre: string;
    hp: number;
    habilidades: string[]
    puebloNatal?: string;
}
const personaje: Personaje = {
    nombre : 'straider',
    hp : 100,
    habilidades: ['Bash', 'Counter', 'Healing'],
}

personaje.puebloNatal = 'Pueblo Paleta';
console.table(personaje)