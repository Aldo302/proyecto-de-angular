const SuperHeroe = {
    nombre: 'Spiderman',
    edad: 30,
    direccion: {
        calle: 'Main St',
        pais: 'USA',
        ciudad: 'NY'
    },
    mostrarDireccion (){
        return this.nombre + ',' + this.direccion.ciudad + ',' + this.direccion.pais;
    }
}

const direccion = SuperHeroe.mostrarDireccion();
console.log (direccion);

interface superHeroe {
    nombre: 'Spiderman';
}

const superHeroe = {
    nombre: 'Spiderman',
    edad: 30,
    direccion: {
        calle: 'Main St',
        pais: 'USA',
        ciudad: 'NY'
    },
    mostrarDireccion (){
        return this.nombre + ',' + this.direccion.ciudad + ',' + this.direccion.pais;
    },
}